net: "models/pre_train.prototxt"
#test_iter: 20
#test_interval: 1000

base_lr: 0.001
lr_policy: "multistep"
gamma: 0.1

#8epoch for casia softmax
stepvalue: 30000
max_iter: 50000

display: 100
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "output/casia_webface_softmax/s2_vgg_casia_softmax"
solver_mode: GPU
